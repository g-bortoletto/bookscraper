{
  "Categories": [
    "Mystery",
    "Fiction",
    "Nonfiction",
    "music"
  ],
  "Filters": {
    "MinPrice": 20.0,
    "MaxPrice": 50.0,
    "Rating": 4
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/bookscraper-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "ApplicationName": "BookScraperApp"
    }
  }
}

